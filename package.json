{
  "name": "type-assert-object",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm-run-all -s type clean build:rollup",
